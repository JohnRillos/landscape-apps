    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "io.tlon.talk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
